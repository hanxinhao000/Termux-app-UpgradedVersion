<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">





    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@color/color_553E3E3E"
        app:cardCornerRadius="5dp"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/jiandan_kongxia_linux_click"
            android:layout_height="120dp">

           <LinearLayout
               android:layout_width="wrap_content"
               android:orientation="vertical"
               android:gravity="center"
               android:layout_centerVertical="true"
               android:layout_centerHorizontal="true"
               android:layout_height="wrap_content">

               <ImageView
                   android:layout_width="40dp"
                   android:layout_height="40dp"
                   android:src="@drawable/qunyou_1" />


               <main.java.com.termux.utils.CustomTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="3dp"
                   android:layout_weight="1"
                   android:gravity="center"
                   android:text="@string/由空夏提供的56456dfi"
                   android:textColor="#fff"
                   android:textSize="13sp"
                   android:textStyle="bold" />

               <main.java.com.termux.utils.CustomTextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="3dp"
                   android:layout_weight="1"
                   android:gravity="center"
                   android:text="@string/s564由空夏提供的56dfi"
                   android:textColor="#fff"
                   android:textSize="10sp"
                   android:textStyle="bold" />

           </LinearLayout>



            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                app:cardElevation="0dp"
                app:cardBackgroundColor="@color/color_553E3E3E"
                app:cardCornerRadius="5dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:background="@color/color_FFFFFF"
                    android:src="@drawable/jiandan_xiazai_xia" />

            </androidx.cardview.widget.CardView>


        </RelativeLayout>



    </androidx.cardview.widget.CardView>



    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:id="@+id/atilo"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@color/color_553E3E3E"
        app:cardCornerRadius="5dp"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="120dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/meihua_group" />


                <main.java.com.termux.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="atilo"
                    android:textColor="#fff"
                    android:textSize="13sp"
                    android:textStyle="bold" />

                <main.java.com.termux.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/使用atilo"
                    android:textColor="#fff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

            </LinearLayout>



            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                app:cardElevation="0dp"
                app:cardBackgroundColor="@color/color_553E3E3E"
                app:cardCornerRadius="5dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:background="@color/color_FFFFFF"
                    android:src="@drawable/jiandan_xiazai_xia" />

            </androidx.cardview.widget.CardView>


        </RelativeLayout>



    </androidx.cardview.widget.CardView>




    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@color/color_553E3E3E"
        app:cardCornerRadius="5dp"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/jiandan_xom_linux_click"
            android:layout_height="120dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:src="@drawable/yuan_code" />


                <main.java.com.termux.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/XOM工具脚本sdf"
                    android:textColor="#fff"
                    android:textSize="13sp"
                    android:textStyle="bold" />

                <main.java.com.termux.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/由XOM作者提供gfg"
                    android:textColor="#fff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

            </LinearLayout>



            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                app:cardElevation="0dp"
                app:cardBackgroundColor="@color/color_553E3E3E"
                app:cardCornerRadius="5dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:background="@color/color_FFFFFF"
                    android:src="@drawable/jiandan_lixian_xia" />

            </androidx.cardview.widget.CardView>


        </RelativeLayout>



    </androidx.cardview.widget.CardView>





    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="5dp"
        android:visibility="gone"
        android:layout_marginRight="5dp"
        app:cardElevation="0dp"
        app:cardBackgroundColor="@color/color_553E3E3E"
        app:cardCornerRadius="5dp"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/jiandan_my_world_linux_click"
            android:layout_height="120dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:layout_height="wrap_content">

                <RelativeLayout
                    android:layout_width="40dp"
                    android:layout_height="40dp">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:src="@drawable/my_world" />

                    <TextView
                        android:layout_width="match_parent"
                        android:background="#88000000"
                        android:visibility="gone"
                        android:layout_height="match_parent"/>

                </RelativeLayout>


                <main.java.com.termux.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/我的世界服务器"
                    android:textColor="#fff"
                    android:textSize="13sp"
                    android:textStyle="bold" />

                <main.java.com.termux.utils.CustomTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="3dp"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="@string/我的世界服务器提供"
                    android:textColor="#fff"
                    android:textSize="10sp"
                    android:textStyle="bold" />

            </LinearLayout>



            <androidx.cardview.widget.CardView
                android:layout_width="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                app:cardElevation="0dp"
                app:cardBackgroundColor="@color/color_553E3E3E"
                app:cardCornerRadius="5dp"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"

                    android:background="@color/color_FFFFFF"
                    android:src="@drawable/jiandan_lixian_xia" />

            </androidx.cardview.widget.CardView>


        </RelativeLayout>



    </androidx.cardview.widget.CardView>











</LinearLayout>
